class Solution {
    public int centeredSubarrays(int[] nums) {
        int n = nums.length;
        int count = 0;

        for(int i = 0;i < n;i++){
            Set<Integer> set = new HashSet<>();
            int preSum = 0;
            
            for(int j = i;j < n;j++){
                set.add(nums[j]);
                preSum += nums[j];
                if(set.contains(preSum)) count++;
            }
        }
        return count;
    }
}
